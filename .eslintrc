{
  "extends": ["airbnb", "airbnb-flow"],
  "rules": {
      "react/jsx-indent": 0,
      "react/jsx-filename-extension": 0,
      "linebreak-style": 0,
      "react/prefer-stateless-function": 0,
      "no-undef": 0,
      "react/jsx-no-undef":0,
      "indent": ["error", "tab"],
	  "no-tabs": 0,
	  "global-require": 0,
	  "import/newline-after-import": 0
  }
}
